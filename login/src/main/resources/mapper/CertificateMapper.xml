<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.demo.security.dao.CertificateMapper">
  <resultMap id="BaseResultMap" type="org.demo.security.entity.Certificate">
    <id column="certificateId" jdbcType="BIGINT" property="certificateId" />
    <result column="country" jdbcType="VARCHAR" property="country" />
    <result column="idNumber" jdbcType="VARCHAR" property="idNumber" />
    <result column="certificateStartDate" jdbcType="VARCHAR" property="certificateStartDate" />
    <result column="certificateEndDate" jdbcType="VARCHAR" property="certificateEndDate" />
    <result column="certificateTypeId" jdbcType="INTEGER" property="certificateTypeId" />
  </resultMap>


  <insert id="addCertificate" useGeneratedKeys="true" keyProperty="certificateId">
    INSERT INTO `certificate`(`country`,
                       `idNumber`,
                       `certificateStartDate`,
                       `certificateEndDate`,
                       `certificateTypeId`) VALUES ( #{certificate.country},
                                             #{certificate.idNumber},
                                             #{certificate.certificateStartDate},
                                             #{certificate.certificateEndDate},
                                             ${certificate.certificateTypeId});
  </insert>
</mapper>